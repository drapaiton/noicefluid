{
    "files.exclude": {
        // not important
        "**/LICENSE": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__/": true,
        // important
        "**/.git*": true,
        "**/.vscode/": true,
        "**/__init__.py": true,
        "**/migrations/": true,
        "**/requirements.*": true,
        // text
        "**/*.txt": true,
        "**/*.md": true,
        "**/readme.md": true
    }
}